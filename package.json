{
  "name": "python-visualization",
  "displayName": "python-visualization",
  "description": "This extension is visualizing the execution of python programs",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:python-visualization.startDebugSession"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Python Visualization",
      "properties": {
        "python-visualization.outputBackendTrace": {
          "type": "boolean",
          "default": false,
          "description": "Creates a file with backend Trace (for debugging purposes)"
        }
      }
    },
    "commands": [
      {
        "command": "python-visualization.startDebugSession",
        "title": "Python Visualization"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "python-visualization.startDebugSession",
          "when": "editorLangId == python && resourceExtname == .py",
          "group": "pythonViz@0"
        }
      ],
      "editor/title/run": [
        {
          "command": "python-visualization.startDebugSession",
          "when": "resourceExtname == .py",
          "group": "pythonViz@0"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/node": "^18.11.13",
    "leader-line": "^1.0.7",
    "sinon": "^15.1.0",
    "stringify-json": "^2.0.0",
    "ts-md5": "^1.3.1"
  }
}
